programa
{
	
	funcao inicio()
	{
		inteiro n1, cont
		escreva("Qual o numero?")
		leia(n1)
		logico primo = verdadeiro

		para(cont = 2; cont < n1; cont++){
			se (n1 % cont == 0){
			primo = falso
			escreva("nao e primo")
			pare
			}
			se (primo == verdadeiro){
				escreva("Ã© primo")
			pare
			}
		}
	}
}
