programa
{
	
	funcao inicio()
	{
		inteiro numero, contador
		logico a = verdadeiro

		escreva("Digite um numero ")
		leia(numero)
		para(contador = 2; contador < numero; contador++){
			se (numero % contador == 0){
				a = falso
				pare
			}
		}
		se (a)
		escreva("primo")
		senao
		escreva("nao primo")
	}
}
